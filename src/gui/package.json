{
  "name": "dreamtime-gui",
  "version": "1.0.0",
  "main": "electron/index.js",
  "author": "Iv√°n Bravo Bravo <ivan@dreamnet.tech>",
  "homepage": "https://time.dreamnet.tech",
  "license": "GPL-3.0-only",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/private-dreamnet/dreamtime.git"
  },
  "bugs": {
    "url": "https://github.com/private-dreamnet/dreamtime/issues"
  },
  "build": {
    "appId": "com.kolessios.dreamtime",
    "productName": "DreamTime",
    "directories": {
      "output": "../../dist/gui-unpacked"
    },
    "win": {
      "target": "dir",
      "icon": "app.ico"
    },
    "linux": {
      "target": "dir",
      "icon": "app.ico",
      "executableName": "DreamTime"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "start:dev:gui": "cross-env NODE_ENV=dev nuxt",
    "start:dev:electron": "cross-env NODE_ENV=dev DEBUG=app:* electron .",
    "build:gui": "cross-env NODE_ENV=production nuxt build",
    "build:electron": "cross-env NODE_ENV=production electron-builder",
    "test": "yarn build:gui && cross-env NODE_ENV=production DEBUG=app:* electron .",
    "build": "yarn build:gui && yarn build:electron",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "aws-sdk": "^2.503.0",
    "bullet-train-nodejs": "^0.1.2",
    "clipboard": "^2.0.4",
    "compare-versions": "^3.5.0",
    "cropperjs": "^1.5.2",
    "debug": "^4.1.1",
    "deferred": "^0.7.11",
    "electron-context-menu": "^0.13.0",
    "electron-dl": "^1.14.0",
    "electron-nucleus": "^2.1.0",
    "electron-utils": "^3.0.0",
    "filesize": "^4.1.2",
    "gpu-info": "^3.0.0",
    "gsap": "^2.1.3",
    "js-event-bus": "^1.0.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "postinstall-postinstall": "^2.0.0",
    "queue": "^6.0.1",
    "randomcolor": "^0.5.4",
    "randomstring": "^1.1.5",
    "raw-loader": "^3.0.0",
    "regedit": "^3.0.3",
    "rookout": "^0.1.67",
    "supports-color": "^7.0.0",
    "sweetalert": "^2.1.2",
    "tippy.js": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "nuxtjs-electron": "^0.1.10",
    "prettier": "^1.16.4",
    "rollbar-sourcemap-webpack-plugin": "^2.5.1",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.0.0"
  }
}
